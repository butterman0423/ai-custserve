{
  "name": "ai-custserve",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p front_dev back_dev",
    "build": "next build && tsc --project src/tsconfig.json",
    "start": "npm-run-all -p front_start back_start",
    "lint": "next lint",
    "front_dev": "next dev",
    "front_start": "next start",
    "back_dev": "tsc --project src/tsconfig.json --watch",
    "back_start": "node src/build/app.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.16.0",
    "dotenv": "^16.4.5",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
